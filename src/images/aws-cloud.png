  <text x="150" y="140" text-anchor="middle" fill="#1f2937" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Abdirahman Mohamed</text>
  <text x="150" y="155" text-anchor="middle" fill="#6b7280" font-family="Arial, sans-serif" font-size="10">has successfully completed the course</text>
  
  <!-- Cloud Icon -->
  <ellipse cx="35" cy="135" rx="15" ry="8" fill="#ff9900" opacity="0.2"/>
  <circle cx="25" cy="135" r="5" fill="#ff9900" opacity="0.3"/>
  <circle cx="35" cy="130" r="6" fill="#ff9900" opacity="0.3"/>
  <circle cx="45" cy="135" r="4" fill="#ff9900" opacity="0.3"/>
  
