  <rect width="300" height="40" fill="url(#awsBg)" rx="10"/>
  <rect x="0" y="30" width="300" height="10" fill="url(#awsBg)"/>
  
  <!-- AWS Logo -->
  <text x="150" y="25" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="16" font-weight="bold">AWS</text>
  
  <!-- Certificate Content -->
  <text x="150" y="65" text-anchor="middle" fill="#1f2937" font-family="Arial, sans-serif" font-size="14" font-weight="bold">Certificate of Completion</text>
  
