  <rect width="300" height="40" fill="url(#courseraBg)" rx="10"/>
  <rect x="0" y="30" width="300" height="10" fill="url(#courseraBg)"/>
  
  <!-- Coursera Logo -->
  <text x="150" y="25" text-anchor="middle" fill="white" font-family="Arial, sans-serif" font-size="16" font-weight="bold">Coursera</text>
  
