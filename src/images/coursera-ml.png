  <text x="150" y="125" text-anchor="middle" fill="#6b7280" font-family="Arial, sans-serif" font-size="10">This is to certify that</text>
  <text x="150" y="140" text-anchor="middle" fill="#1f2937" font-family="Arial, sans-serif" font-size="12" font-weight="bold">Abdirahman Mohamed</text>
  <text x="150" y="155" text-anchor="middle" fill="#6b7280" font-family="Arial, sans-serif" font-size="10">has successfully completed the specialization</text>
  
  <!-- ML Icon -->
  <circle cx="35" cy="135" r="12" fill="#0056d3" opacity="0.2"/>
  <circle cx="30" cy="130" r="3" fill="#0056d3"/>
  <circle cx="35" cy="135" r="3" fill="#0056d3"/>
  <circle cx="40" cy="140" r="3" fill="#0056d3"/>
  <line x1="30" y1="130" x2="35" y2="135" stroke="#0056d3" stroke-width="2"/>
  <line x1="35" y1="135" x2="40" y2="140" stroke="#0056d3" stroke-width="2"/>
  
